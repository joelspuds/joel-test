{% extends 'layouts/mts-without-container.layout.njk' %}
 {% set pageTitle = 'homepage' %}
 {% set pageBreadcrumbItems = [
  { text: 'Home', url: '/prototypes' },
  { text: 'MOT test results', url: '/prototypes/mot-test' },
  { text: 'Brake test' }
] %}
 {% block content %}
 <main id="content">
    
    <a href="/prototypes/recalls/V1/vehicle-model" class="link-back">Back</a> 

    <div class="error-summary" role="alert" aria-labelledby="error-summary-example-heading-one" tabindex="-1">
        <h2 class="heading-medium error-summary-heading" id="error-summary-example-heading-one">
            There was a problem
        </h2>
        <ul class="error-summary-list">
            <li>
                <a href="#example-personal-details">
                    Enter a valid year of manufacture
                </a>
            </li>
        </ul>
    </div>

     <header class="content-header">
        <h1 class="heading-large heading heading--page-heading heading--small-margin-bottom">
                When was the vehicle manufactured?</h1>
    </header>                       
     <div class="row contents">
        <div class="col-lg-12">
     <div class="grid-row">
    <div class="column-three-quarters">
        <div class="form-group form-group-error">
            <label class="form-label" for="Year-of-manufactuer">
                Year of manufacture
                <span class="form-hint">For example, 2004</span>
                <span class="error-message">Enter a valid year of manufacture</span>
            </label>
            <input class="form-control form-control-1-4" id="Year-of-manufactuer" type="text" name="Year-of-manufactuer" value="">
        </div>
        <br>
        <div>
            <a class="button" id="choose-make" href="/prototypes/recalls/V1/results-page" style="margin-bottom:30px">Continue</a>
        </div>
     </div>
</div>            
 </main>
 {% endblock %} 
