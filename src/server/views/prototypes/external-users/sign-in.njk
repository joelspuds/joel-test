{% set showExternalNav = true %}
{% extends 'layouts/bulma-ufs.layout.njk' %}

{% set isAdminUser = false %}
{%  set loggedOut = true %}

{% set pageTitle = 'Sign in' + GLOBAL_TAG_TITLE_SUFFIX %}

{% block content %}

    {% call components.bulmaRow() %}
        {% call components.bulmaColumnAny(size='is-two-thirds') %}
            {{ components.captionHeading(text='Sign in') }}
        {%- endcall %}
    {%- endcall %}

    {% call components.bulmaRow() %}

        {% call components.bulmaColumnAny(size='column is-full is-9-desktop is-9-widescreen', customID='mainContent') %}

            <p class="govuk-body">
                [sign in form]
                <br><br><br><br><br><br><br><br><br><br><br>
            </p>

            <p class="govuk-body">
                <a href="/prototypes/external-users/home" class="govuk-button">Sign in</a>
            </p>



        {%- endcall %}

    {%- endcall %}


{% endblock %}
{% block bodyScripts %}
{% endblock %}


